
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<br />
<form class="form-group" method="post">
    <input class="form-control" name="Baslik" placeholder="Başlık yazınız" />
    <br />
    <input class="form-control" name="AltBaslik" placeholder="Alt başlık yazınız" />
    <br />
    <input class="form-control" name="Tarih" placeholder="Tarih yazınız" />
    <br />
    <textarea class="form-control" name="Aciklama" placeholder="Açıklama yazınız" rows="5"></textarea>
    <br />
    <button class="btn btn-info" type="submit">Kaydet</button>
</form>

