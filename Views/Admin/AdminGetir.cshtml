@model MvcCv.Models.entity.TblAdmin
@{
    ViewBag.Title = "AdminGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Kullanıcı Düzenle Sayfası</h4>
<br />
<form method="post" class="form-group">
    @Html.Label("Deneyim Id")
    @Html.TextBoxFor(x => x.Id, new { @class = "form-control" })
    @Html.Label("Başlık")
    @Html.TextBoxFor(x => x.KullaniciAdi, new { @class = "form-control" })
    @Html.Label("Alt Başlık")
    @Html.TextBoxFor(x => x.Parola, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Güncelle</button>
</form>